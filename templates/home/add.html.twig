<!-- Modal -->
<div class="modal fade" id="addRangeModal" tabindex="-1" role="dialog" aria-labelledby="addRangeModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <form method="POST" id="new_range">
            {{ form_start(form)}}
            <div class="modal-header">
                <h5 class="modal-title" id="addRangeModalLabel">Ajouter une gamme</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-row">
                    <div class="form-group col-md-8">
                        {{
                            form_row(form.libelle)
                        }}
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group col-md-8">
                        {{
                            form_row(form.userWorkstation)
                        }}
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Fermer</button>
                <button type="submit" class="btn btn-primary">Ajouter</button>
            </div>
            {{ form_end(form) }}
            </form>
        </div>
    </div>
</div>