<div class="modal fade" id="editWorkStationModal{{ workstation.id }}" data-whatever="{{ workstation.id }}" tabindex="-1" role="dialog"  aria-labelledby="editWorkStationModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <form method="POST" id="edit_workstation{{ workstation.id }}" data-whatever="{{ workstation.id }}">
                {{ form_start(form_workstation)}}
                <div class="modal-header">
                    <h5 class="modal-title" id="editWorkStationModalLabel">Modifier un poste de travail </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-row">
                        <div class="form-group col-md-10">
                            {{
                                form_row(form_workstation.libelle)
                            }}
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-10">
                            {{
                                form_row(form_workstation.userWorkstation)
                            }}
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Fermer</button>
                    <button type="submit" class="btn btn-warning">Modifier</button>
                </div>
                {{ form_end(form_workstation) }}
            </form>
        </div>
    </div>
</div>